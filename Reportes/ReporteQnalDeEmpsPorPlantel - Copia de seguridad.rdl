      <Query>
        <DataSourceName>Nomina</DataSourceName>
        <CommandText>--Select Distinct EP.IdPlaza, EP.IdEmp, EP.IdEmpFuncion, Convert(Nvarchar(1), EP.IdEmpFuncion) + P.ClavePlantel + EDL.NumEmp ClaveEmp,
--	E.RFCEmp, IsNull(E.ApePatEmp,'') ApePatEmp, IsNull(E.ApeMatEmp,'') ApeMatEmp, IsNull(E.NomEmp,'') NomEmp,
--	C.IdCategoria, C.ClaveCategoria, C.DescCategoria, EP.IdTipoEmp, TE.DescTipoEmp, TE.AbrevTipoEmp,
--	PH.IdQnaVigIni, dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni) QnaIni, PH.IdQnaVigFin, dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin) QnaFin
--From HistoriaPago HP
--	Inner Join EmpleadosPlazas EP On EP.IdPlaza = HP.IdPlaza
--	Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
--	Inner Join EmpleadosDatosLaborales EDL On EDL.IdEmp = EP.IdEmp
--	Inner Join Planteles P On P.IdPlantel = EP.IdPlantel
--	Inner Join Empleados E On E.IdEmp = EP.IdEmp
--	Inner Join Categorias C On C.IdCategoria = EP.IdCategoria
--	Inner Join TipoEmpleado TE On TE.IdTipoEmp = EP.IdTipoEmp
--Where --Left(dbo.Fn_ObtenQnaPorId(1815),6) Between Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni),6) And Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin),6)
--	HP.IdQuincenaAplicacion = @IdQuincena
--	And EP.IdPlantel = @IdPlantel And Ep.IdEmpFuncion = 1
--Order by EP.IdEmpFuncion, C.ClaveCategoria, ClaveEmp, ApePatEmp, ApeMatEmp, NomEmp

Select Distinct EP.IdPlaza, EP.IdEmp, EP.IdEmpFuncion, Convert(Nvarchar(1), EP.IdEmpFuncion) + P.ClavePlantel + EDL.NumEmp ClaveEmp,
	E.RFCEmp, IsNull(E.ApePatEmp,'') ApePatEmp, IsNull(E.ApeMatEmp,'') ApeMatEmp, IsNull(E.NomEmp,'') NomEmp,
	C.IdCategoria, C.ClaveCategoria, C.DescCategoria, EP.IdTipoEmp, TE.DescTipoEmp, TE.AbrevTipoEmp,
	PH.IdQnaVigIni, dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni) QnaIni, PH.IdQnaVigFin, dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin) QnaFin
From EmpleadosPlazas EP
	Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
	Inner Join EmpleadosDatosLaborales EDL On EDL.IdEmp = EP.IdEmp
	Inner Join Planteles P On P.IdPlantel = EP.IdPlantel
	Inner Join Empleados E On E.IdEmp = EP.IdEmp
	Inner Join Categorias C On C.IdCategoria = EP.IdCategoria
	Inner Join TipoEmpleado TE On TE.IdTipoEmp = EP.IdTipoEmp
Where Left(dbo.Fn_ObtenQnaPorId(@IdQuincena),6) Between Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni),6) And Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin),6)
	And EP.IdPlantel = @IdPlantel And Ep.IdEmpFuncion = 1
Order by EP.IdEmpFuncion, C.ClaveCategoria, ClaveEmp, ApePatEmp, ApeMatEmp, NomEmp</CommandText>
