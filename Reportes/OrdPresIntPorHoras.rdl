        <CommandText>Declare @IdOrigenPropuesta Tinyint
Declare @IdTipoSemestre Tinyint --18/10/2012

Select @IdOrigenPropuesta = IdOrigenPropuesta
From OrdenesDePresentacion
Where IdOrdenDePres = @IdOrdenDePres

--18/10/2012
Select @IdTipoSemestre = ISNULL(ST.IdTipoSemestre,3)
From Semestres S
	Inner Join SemestresTipos ST On ST.TipoSemestre = S.TipoSemestre
Where @IdQnaIni Between IdQuincenaIni And IdQuincenaFin
--18/10/2012

Declare @IdCT Smallint

Select @IdCT = EP.IdCT
From EmpleadosPlazas EP
	Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
Where Left(dbo.Fn_ObtenQnaPorId(@IdQnaIni),6) Between Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni),6) And Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin),6)
	And EP.PlazaEspecial = 0 And EP.IdEmp = @IdEmp
	And EP.IdTipoSemestre In (@IdTipoSemestre,3) --18/10/2012

If Exists
	(Select 1
	From CentrosDeTrabajo
	Where IdCT = @IdCT And IdTipoCT = 7)
	Begin
		Select @IdCT = CT.IdCT
		From OrdenesDePresentacionHoras OPPH
			Inner Join Horas H On H.IdHora = OPPH.IdHora
			Inner Join Planteles P On P.IdPlantel = H.IdPlantel
			Inner Join CentrosDeTrabajo CT On CT.IdPlantel = P.IdPlantel
		Where OPPH.IdOrdenDePres = @IdOrdenDePres
			And @IdQnaIni Between P.IdQnaIni And P.IdQnaFin
			And @IdQnaIni Between CT.IdQnaIni And CT.IdQnaFin

		Select LTRIM(RTRIM(IsNull(P.AbrevProfesion, 'LIC.'))) Profesion,
			LTRIM(RTRIM(IsNull(E.NomEmp,'') + ' ' +  IsNull(E.ApePatEmp,'') + ' ' + IsNull(E.ApeMatEmp,''))) NomEmp,
			CT.DescDelTitular Cargo, 
			Case
				When CT.IdTipoCT = 7 Then 'Plantel ' + Right(CT.ClaveCT,2) + '-' + CT.NombreCT
				Else CT.NombreCT
			End NombreCT, CT.IdTipoCT
		From EmpleadosPlazas EP
			Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
			Inner Join CentrosDeTrabajo CT On CT.IdCT = EP.IdCT
			Inner Join Empleados E On E.IdEmp = EP.IdEmp
			Left Join EmpleadosDatosAcademicos EDA On EDA.IdEmp = E.IdEmp
			Left Join Profesiones P On P.IdProfesion = EDA.IdProfesion
		Where --Left(dbo.Fn_ObtenQnaPorId(@IdQnaIni),6) Between Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni),6) And Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin),6)
			@IdQnaIni Between PH.IdQnaVigIni And PH.IdQnaVigFin
			And EP.IdFuncionPri Not In (6,8,9,10,11) And EP.IdTipoNomina = 1
			And EP.IdCT = @IdCT
			And EP.IdTipoSemestre In (@IdTipoSemestre,3) --18/10/2012
		Order By  CT.DescDelTitular
	End
Else
	Begin
		If Exists
			(Select 1
			From CentrosDeTrabajo
			Where IdCT = @IdCT And IdTipoCT = 8)
			Begin	
				Select LTRIM(RTRIM(IsNull(P.AbrevProfesion, 'LIC.'))) Profesion,
					RTRIM(LTRIM(IsNull(E.NomEmp,'') + ' ' + IsNull(E.ApePatEmp,'') + ' ' + IsNull(E.ApeMatEmp,''))) NomEmp,
					Case
						When  Right(Left(E.CURPEmp,11),1) = 'M' Then 'SECRETARIA GENERAL DEL ' + LTRIM(RTRIM(S.SiglasSindicato))
						When  Right(Left(E.CURPEmp,11),1) = 'H' Then 'SECRETARIO GENERAL DEL ' + LTRIM(RTRIM(S.SiglasSindicato))
					End Cargo, 'COMITE EJECUTIVO SUIT' NombreCT, 8 IdTipoCT
				From HistoriaLideresSindicales HLS
					Inner Join Sindicatos S On S.IdSindicato = HLS.IdSindicato
					Inner Join Empleados E On E.IdEmp = HLS.IdEmp
					Left Join EmpleadosDatosAcademicos EDA On EDA.IdEmp = E.IdEmp
					Left Join Profesiones P On P.IdProfesion = EDA.IdProfesion
				Where HLS.IdSindicato = @IdOrigenPropuesta And
					Left(dbo.Fn_ObtenQnaPorId(@IdQnaIni),6) Between Left(dbo.Fn_ObtenQnaPorId(HLS.IdQnaIni),6) And Left(dbo.Fn_ObtenQnaPorId(HLS.IdQnaFin),6)	
			End
		Else
			Begin
				If Exists
					(
						Select 1
						From EmpleadosPlazas EP
							Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
							Inner Join CentrosDeTrabajo CT On CT.IdCT = EP.IdCT
							Inner Join Empleados E On E.IdEmp = EP.IdEmp
							Left Join EmpleadosDatosAcademicos EDA On EDA.IdEmp = E.IdEmp
							Left Join Profesiones P On P.IdProfesion = EDA.IdProfesion
						Where Left(dbo.Fn_ObtenQnaPorId(@IdQnaIni),6) Between Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni),6) And Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin),6)
							And EP.IdFuncionPri Not In (6,8,9,10,11) And EP.IdTipoNomina &lt;&gt; 5
							And EP.IdCT = @IdCT
							And EP.IdTipoSemestre In (@IdTipoSemestre,3) --18/10/2012
						--Order By  CT.DescDelTitular					
					)
					Begin
						Select LTRIM(RTRIM(IsNull(P.AbrevProfesion, 'LIC.'))) Profesion,
							LTRIM(RTRIM(IsNull(E.NomEmp,'') + ' ' +  IsNull(E.ApePatEmp,'') + ' ' + IsNull(E.ApeMatEmp,''))) NomEmp,
							CT.DescDelTitular Cargo, 
							Case
								When CT.IdTipoCT = 7 Then 'Plantel ' + Right(CT.ClaveCT,2) + '-' + CT.NombreCT
								Else CT.NombreCT
							End NombreCT, CT.IdTipoCT
						From EmpleadosPlazas EP
							Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
							Inner Join CentrosDeTrabajo CT On CT.IdCT = EP.IdCT
							Inner Join Empleados E On E.IdEmp = EP.IdEmp
							Left Join EmpleadosDatosAcademicos EDA On EDA.IdEmp = E.IdEmp
							Left Join Profesiones P On P.IdProfesion = EDA.IdProfesion
						Where Left(dbo.Fn_ObtenQnaPorId(@IdQnaIni),6) Between Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni),6) And Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin),6)
							And EP.IdFuncionPri Not In (6,8,9,10,11) And EP.IdTipoNomina &lt;&gt; 5
							And EP.IdCT = @IdCT
							And EP.IdTipoSemestre In (@IdTipoSemestre,3) --18/10/2012
						Order By  CT.DescDelTitular
					End
				Else
					Begin
						Select LTRIM(RTRIM(IsNull(P.AbrevProfesion, 'LIC.'))) Profesion,
							LTRIM(RTRIM(IsNull(E.NomEmp,'') + ' ' +  IsNull(E.ApePatEmp,'') + ' ' + IsNull(E.ApeMatEmp,''))) NomEmp,
							CT.DescDelTitular Cargo, 
							Case
								When CT.IdTipoCT = 7 Then 'Plantel ' + Right(CT.ClaveCT,2) + '-' + CT.NombreCT
								Else CT.NombreCT
							End NombreCT, CT.IdTipoCT
						From EmpsSoloCompenPlazas EP
							--Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
							Inner Join CentrosDeTrabajo CT On CT.IdCT = EP.IdCT
							Inner Join EmpleadosDatosLaborales EDL On EDl.NumEmp = EP.NumEmp
							Inner Join Empleados E On E.IdEmp = EDL.IdEmp
							Left Join EmpleadosDatosAcademicos EDA On EDA.IdEmp = E.IdEmp
							Left Join Profesiones P On P.IdProfesion = EDA.IdProfesion
						Where Left(dbo.Fn_ObtenQnaPorId(@IdQnaIni),6) Between Left(dbo.Fn_ObtenQnaPorId(EP.IdQuincenaIni),6) And Left(dbo.Fn_ObtenQnaPorId(EP.IdQuincenaFin),6)
							And EP.IdFuncionPri Not In (6,8,9,10,11) --And EP.IdTipoNomina &lt;&gt; 5
							And EP.IdCT = @IdCT
							--And EP.IdTipoSemestre In (@IdTipoSemestre,3) --18/10/2012
						Order By  CT.DescDelTitular					
					End
			End
	End</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Profesion">
          <DataField>Profesion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NomEmp">
