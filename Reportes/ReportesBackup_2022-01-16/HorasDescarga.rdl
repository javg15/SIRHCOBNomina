  <DataSets>
    <DataSet Name="dsHorasDescarga">
      <Query>
        <DataSourceName>dsNomina</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdSemestre">
            <Value>=Parameters!pIdSemestre.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select P.ClavePlantel, P.DescPlantel, P.IdZonaGeografica, EDL.NumEmp, E.NomEmp, IsNull(E.ApePatEmp,'') ApePatEmp, IsNull(E.ApeMatEmp,'') ApeMatEmp, 
	M.ClaveMateria, M.NombreMateria, H.Cantidad HorasMateria
From Horas H
	Inner Join Planteles P On P.IdPlantel = H.IdPlantel
	Inner Join Empleados E On E.IdEmp = H.IdEmp
	Inner Join EmpleadosDatosLaborales EDL On EDL.IdEmp = H.IdEmp
	Inner Join Materias M On M.IdMateria = H.IdMateria
Where H.IdSemestre = @IdSemestre
	And H.IdMateria In
					(
						Select IdMateria
						From Materias
						Where Descarga = 1 Or HorasMateria = 1 Or (ClaveMateria Between '4000' And '4999' And HorasMateria = 1)
					)
	And H.IdEstatusHora = 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
