  <DataSets>
    <DataSet Name="dsTitulo">
      <Query>
        <DataSourceName>Nomina</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdMesValue">
            <Value>=Parameters!pIdMes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Anio">
            <Value>=Parameters!pAnio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InfComp">
            <Value>=Parameters!pInfComp.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Declare @NombreMes Nvarchar(15)
Declare @IdMes Tinyint
Declare @Adicional Tinyint
Declare @Comentarios Nvarchar(100)
DECLARE @TipoPago NVARCHAR(5)

Set @IdMes = Convert(Tinyint, Left(@IdMesValue,2))
Set @Adicional = Convert(Tinyint, Right(@IdMesValue,2)) 
Set @Comentarios = ''

Select @NombreMes = UPPER(NombreMes)
From Meses
Where IdMes = @IdMes

SELECT @TipoPago = 
	CASE
		WHEN RTRIM(LTRIM(AMC.Comentarios)) = '' THEN 'O'
		WHEN AMC.Comentarios IS NULL THEN 'O'
		WHEN AMC.Complementaria = 1 THEN 'C'
		WHEN AMC.MesNormal = 1 THEN 'O'
		ELSE 'E'
	END
From AniosMesesCompensaciones AMC 
WHERE AMC.Anio = @Anio AND AMC.IdMes = @IdMes
	AND AMC.Adicional = @Adicional

IF @InfComp = 'G'
	BEGIN
		SELECT @Adicional = 0

		IF @TipoPago = 'E'
			BEGIN
				Select TOP 1 @Comentarios = 
					Case 
						When Comentarios Is Null Then ''
						When LTrim(RTrim(Comentarios)) = '' Then ''
						Else ' (' + LTrim(RTrim(Upper(Comentarios))) + ')'
					End
				From AniosMesesCompensaciones
				Where Anio = @Anio And IdMes = @IdMes
					AND 
						CASE
							WHEN RTRIM(LTRIM(Comentarios)) = '' THEN 'O'
							WHEN Comentarios IS NULL THEN 'O'
							WHEN Complementaria = 1 THEN 'C'
							WHEN MesNormal = 1 THEN 'O'
							ELSE 'E'
						END	= @TipoPago
				Order By Adicional ASC
			END
	END

If @Adicional &lt;&gt; 0
	Begin
		Select @Comentarios = 
			Case 
				When Comentarios Is Null Then ''
				When LTrim(RTrim(Comentarios)) = '' Then ''
				Else ' (' + LTrim(RTrim(Upper(Comentarios))) + ')'
			End
		From AniosMesesCompensaciones
		Where Anio = @Anio And IdMes = @IdMes And Adicional = @Adicional
	End

Select 'COMPENSACIONES CORRESPONDIENTES AL MES DE ' + @NombreMes + ' DE ' + Convert(Nvarchar(4), @Anio) + @Comentarios  TITULOREPORTE</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
