            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @IdMes Tinyint

SET  @IdMes = CONVERT(Tinyint, LEFT(@IdMesValue, 2))

SELECT MAX(FechaDePago) Fecha
FROM     AniosMesesCompensaciones
WHERE  Anio = @Anio AND IdMes = @IdMes</CommandText>
