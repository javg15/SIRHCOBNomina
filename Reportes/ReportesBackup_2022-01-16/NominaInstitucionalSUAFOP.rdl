        <Field Name="TituloNomina">
          <DataField>TituloNomina</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FechaDePago">
          <DataField>FechaDePago</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
