    <DataSet Name="dsQuincena">
      <Query>
        <DataSourceName>Nomina</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Anio">
            <Value>=Parameters!pAnio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IdMes">
            <Value>=Parameters!pMes.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Declare @NombreMes Nvarchar(15)

Select @NombreMes = UPPER(NombreMes)
From Meses
Where IdMes = @IdMes

IF @IdMes IS NULL
	BEGIN
		SELECT 'REPORTE INSTITUCIONAL DE PERCEPCIONES Y DEDUCCIONES, ' + CONVERT(Nvarchar(4),@Anio)  TITULOREPORTE
	END
ELSE
	BEGIN
		SELECT 'REPORTE INSTITUCIONAL DE PERCEPCIONES Y DEDUCCIONES, ' + @NombreMes + ' ' + CONVERT(Nvarchar(4),@Anio) TITULOREPORTE
	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
