        <CommandText>Select RTRIM(LTRIM(E.NomEmp + ' ' + E.ApePatEmp + ' ' + E.ApeMatEmp)) NomEmp, Right(Left(E.CURPEmp,11),1) Sexo,
	Case
		When TE.AbrevTipoEmp = 'I' Then 'Interino'
		When TE.AbrevTipoEmp = 'P' Then 'Provisional'
		Else ''
	End TipoOcupacion,
	TE.AbrevTipoEmp,
	LTRIM(RTRIM(C.DescCategoria)) Categoria,
	CT.IdCT, CT.NombreCT, EP.IdSindicato, EP.IdEmpFuncion, EP.IdEmp
From EmpleadosPlazas EP
	Inner Join Empleados E On E.IdEmp = EP.IdEmp
	Inner Join TipoEmpleado TE On TE.IdTipoEmp = EP.IdTipoEmp
	Inner Join Categorias C On C.IdCategoria = EP.IdCategoria
	Inner Join CentrosDeTrabajo CT On CT.IdCT = EP.IdCT
Where EP.IdPlaza = @IdPlaza</CommandText>
