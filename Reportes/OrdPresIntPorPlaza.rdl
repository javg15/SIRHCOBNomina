        <CommandText>/*
Declare @IdEmpTitular Int

Select @IdEmpTitular = IdEmpTitular
From PlazasInterinas
Where IdPlaza = @IdPlaza

Select RTRIM(LTRIM(E.NomEmp + ' ' + E.ApePatEmp + ' ' + E.ApeMatEmp)) NomEmp,
	Replace(MGB.DescMotGralBaja, 'BAJA POR ', '') DescMotGralBaja
From EmpleadosPlazas EP
	Inner Join CadenasPlazas CP On CP.IdPlaza = EP.IdPlaza
	Inner Join Empleados E On E.IdEmp = EP.IdEmp
	Inner Join PlazasMotivosDeBaja PMB On PMB.IdPlaza = CP.IdPlaza
	Inner Join MotivosGeneralesDeBaja MGB On MGB.IdMotGralBaja = PMB.IdMotGralBaja
Where EP.IdEmp = @IdEmpTitular And CP.TipoMov = 'B' And CP.IdCadena = @IdCadena
*/
Select RTRIM(LTRIM(E.NomEmp + ' ' + E.ApePatEmp + ' ' + E.ApeMatEmp)) NomEmp,
	UPPER(MHI.DescParaFormatos) DescMotGralBaja
From PlazasInterinas PIn
	Inner Join Empleados E On E.IdEmp = PIn.IdEmpTitular
	Inner Join MotivosHorasInterinas MHI On MHI.IdMotivoHoraInterina = PIn.IdMotivoInterinato
Where PIn.IdPlaza = @IdPlaza</CommandText>
