    <DataSet Name="dsEmpleados">
      <Query>
        <DataSourceName>Nomina</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdQuincena">
            <Value>=Parameters!pIdQuincena.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select
	P.IdPlantel, P.ClavePlantel, P.ClavePlantel + ' - ' + P.DescPlantel Plantel,
	EDL.NumEmp, E.RFCEmp, E.CURPEmp, LTRIM(RTRIM(ISNULL(E.ApePatEmp,'') + ' ' + ISNULL(E.ApeMatEmp,'') + ' ' + E.NomEmp)) NomEmp,
	PD.IdQnaVigIniDeduc, DBO.Fn_ObtenQnaPorId(PD.IdQnaVigIniDeduc) Inicio,
	PD.IdQnaVigFinDeduc, DBO.Fn_ObtenQnaPorId(PD.IdQnaVigFinDeduc) Fin,
	CASE 
		WHEN HPT.IdEmp IS NOT NULL THEN 'VIGENTE EN QUINCENA ' + DBO.Fn_ObtenQnaPorId(@IdQuincena)
		ELSE 'NO VIGENTE Y/O QUINCENA ' + DBO.Fn_ObtenQnaPorId(@IdQuincena) + ' NO CALCULADA'
	End EstatusEmpleado
From PlazasDeducciones PD
	Inner Join EmpleadosPlazas EP ON EP.IdPlaza = PD.IdPlaza
	Inner Join Empleados E On E.IdEmp = EP.IdEmp
	Inner Join EmpleadosDatosLaborales EDL On EDL.IdEmp = EP.IdEmp
	Inner Join Planteles P On P.IdPlantel = EDL.IdPlantel
	Left Join HistoriaPagoTotales HPT On HPT.IdEmp = EP.IdEmp 
										AND HPT.IdQuincenaAplicacion = @IdQuincena
										AND  HPT.IdEmp NOT IN (SELECT IdEmp FROM TblEmpsNoVis)
Where @IdQuincena BETWEEN PD.IdQnaVigIniDeduc AND PD.IdQnaVigFinDeduc
	AND PD.IdDeduccion = 139
Order By P.ClavePlantel, NomEmp</CommandText>
      </Query>
