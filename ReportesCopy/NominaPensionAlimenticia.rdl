    <DataSet Name="dsNominaPensionAlimenticia">
      <Query>
        <DataSourceName>Nomina</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdQuincena">
            <Value>=Parameters!pIdQuincena.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IdPlantel">
            <Value>=Parameters!pIdPlantel.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IdZonaGeografica">
            <Value>=Parameters!pIdZonaGeografica.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Declare @IdTipoSemestre Tinyint --18/10/2012
Declare @IdQuincenaAux Smallint
Declare @Anio Smallint
Declare @Quincena Smallint

--18/10/2012
If dbo.Fn_ObtenSiQnaEsExtraordinaria(@IdQuincena) = 0
	Begin
		Select @IdTipoSemestre = ISNULL(ST.IdTipoSemestre,3)
		From Semestres S
			Inner Join SemestresTipos ST On ST.TipoSemestre = S.TipoSemestre
		Where @IdQuincena Between IdQuincenaIni And IdQuincenaFin
	End
Else
	Begin
		Set @Anio = dbo.Fn_ObtenAnioQna(@IdQuincena)
		Set @Quincena = dbo.Fn_ObtenQna(@IdQuincena)
	
		Select @IdQuincenaAux = IdQuincena
		From Quincenas
		Where Anio = @Anio And Quincena = @Quincena And Adicional = 0
	
		Select @IdTipoSemestre = ISNULL(ST.IdTipoSemestre,3)
		From Semestres S
			Inner Join SemestresTipos ST On ST.TipoSemestre = S.TipoSemestre
		Where @IdQuincenaAux Between IdQuincenaIni And IdQuincenaFin		
	End
--18/10/2012

If @IdPlantel Is Not Null
	Begin
		Select dbo.Fn_ObtenNomEmp(BPA.IdEmp) Empleado, BPA.IdBeneficiario, dbo.Fn_ObtenNombreBeneficiario(BPA.IdBeneficiario) Beneficiario, HPA.Importe,
			dbo.Fn_ObtenPlazaPorId(EP.IdPlaza) Plaza, P.IdPlantel, P.ClavePlantel, P.DescPlantel
		From HistoriaPensionAlimenticia HPA
			Inner Join BeneficiariosPensionAlimenticia BPA On BPA.IdBeneficiario = HPA.IdBeneficiario
			Inner Join Empleados E On E.IdEmp = BPA.IdEmp
			Inner Join EmpleadosPlazas EP On EP.IdEmp = E.IdEmp
			Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
			Inner Join Planteles P On P.IdPlantel = BPA.IdPlantel
		Where HPA.IdQuincenaAplicacion = @IdQuincena And BPA.IdPlantel = @IdPlantel
			And Left(dbo.Fn_ObtenQnaPorId(@IdQuincena),6) Between Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni),6) And Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin),6)
						And EP.PlazaEspecial = 0
			And EP.IdTipoSemestre In (@IdTipoSemestre,3) --18/10/2012                
		Order By Empleado, Beneficiario
	End
Else
	Begin
		If @IdZonaGeografica Is Not Null
			Begin
				Select dbo.Fn_ObtenNomEmp(BPA.IdEmp) Empleado, BPA.IdBeneficiario, dbo.Fn_ObtenNombreBeneficiario(BPA.IdBeneficiario) Beneficiario, HPA.Importe,
					dbo.Fn_ObtenPlazaPorId(EP.IdPlaza) Plaza, P.IdPlantel, P.ClavePlantel, P.DescPlantel
				From HistoriaPensionAlimenticia HPA
					Inner Join BeneficiariosPensionAlimenticia BPA On BPA.IdBeneficiario = HPA.IdBeneficiario
					Inner Join Empleados E On E.IdEmp = BPA.IdEmp
					Inner Join EmpleadosPlazas EP On EP.IdEmp = E.IdEmp
					Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
					Inner Join Planteles P On P.IdPlantel = BPA.IdPlantel
				Where HPA.IdQuincenaAplicacion = @IdQuincena --And BPA.IdPlantel = @IdPlantel
					And P.IdZonaGeografica = @IdZonaGeografica
					And Left(dbo.Fn_ObtenQnaPorId(@IdQuincena),6) Between Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni),6) And Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin),6)
								And EP.PlazaEspecial = 0
					And EP.IdTipoSemestre In (@IdTipoSemestre,3) --18/10/2012                
				Order By P.ClavePlantel, Empleado, Beneficiario
			End
		Else
			Begin
				Select dbo.Fn_ObtenNomEmp(BPA.IdEmp) Empleado, BPA.IdBeneficiario, dbo.Fn_ObtenNombreBeneficiario(BPA.IdBeneficiario) Beneficiario, HPA.Importe,
					dbo.Fn_ObtenPlazaPorId(EP.IdPlaza) Plaza, P.IdPlantel, P.ClavePlantel, P.DescPlantel
				From HistoriaPensionAlimenticia HPA
					Inner Join BeneficiariosPensionAlimenticia BPA On BPA.IdBeneficiario = HPA.IdBeneficiario
					Inner Join Empleados E On E.IdEmp = BPA.IdEmp
					Inner Join EmpleadosPlazas EP On EP.IdEmp = E.IdEmp
					Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
					Inner Join Planteles P On P.IdPlantel = BPA.IdPlantel
				Where HPA.IdQuincenaAplicacion = @IdQuincena --And BPA.IdPlantel = @IdPlantel
					--And P.IdZonaGeografica = @IdZonaGeografica
					And Left(dbo.Fn_ObtenQnaPorId(@IdQuincena),6) Between Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni),6) And Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin),6)
								And EP.PlazaEspecial = 0
					And EP.IdTipoSemestre In (@IdTipoSemestre,3) --18/10/2012                
				Order By P.IdZonaGeografica, P.ClavePlantel, Empleado, Beneficiario			
			End
	End</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
