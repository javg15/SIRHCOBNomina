        </Field>
        <Field Name="DescClasifPercDeduc">
          <DataField>DescClasifPercDeduc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Importe">
          <DataField>Importe</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
