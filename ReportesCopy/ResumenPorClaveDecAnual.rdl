  <DataSets>
    <DataSet Name="dsResumen">
      <Query>
        <DataSourceName>Nomina</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Ejercicio">
            <Value>=Parameters!pEjercicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Mes">
            <Value>=Parameters!pMes.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>If @Mes Is Null
	Begin
		Select *
		From
			(
				Select ClaveConcepto, NombreConcepto, Gravar, ClaveClasifPercDeduc, DescClasifPercDeduc, Sum(Importe) Importe
				From
				(
					Select P.ClavePercepcion ClaveConcepto, P.NombrePercepcion NombreConcepto, P.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, 
						Case
							When A1.Origen In ('DR','DN') Then -A1.Importe
							Else A1.Importe
						End Importe
					From DeclaracionesAnuales DA
						Inner Join AcumuladosPorEjercicioFiscal A1 On A1.IdEmp = DA.IdEmp
						Inner Join Percepciones P On P.IdPercepcion = A1.IdPercDeduc And A1.TipoPercDeduc = 'P'
						Inner Join PercsClasifPercDeducHacienda PH On PH.IdPercepcion = A1.IdPercDeduc
						Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
					Where DA.Ejercicio = @Ejercicio And A1.Ejercicio = @Ejercicio
						And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
						And DA.IdEmp Not In
							(
								Select Distinct IdEmp
								From Bajas_Borrar
								Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
							)				
					Union All
					Select P.ClavePercepcion ClaveConcepto, P.NombrePercepcion NombreConcepto, P.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, A1.Importe
					From DeclaracionesAnuales DA
						Inner Join AcumuladosPorEjercicioFiscal2 A1 On A1.IdEmp = DA.IdEmp
						Inner Join Percepciones P On P.IdPercepcion = A1.IdPercDeduc And A1.TipoPercDeduc = 'P'
						Inner Join PercsClasifPercDeducHacienda PH On PH.IdPercepcion = A1.IdPercDeduc
						Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
					Where DA.Ejercicio = @Ejercicio And A1.Ejercicio = @Ejercicio
						And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
						And DA.IdEmp Not In
							(
								Select Distinct IdEmp
								From Bajas_Borrar
								Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
							)				
					Union All
					Select P.ClavePercepcion ClaveConcepto, P.NombrePercepcion NombreConcepto, P.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, A1.Importe
					From DeclaracionesAnuales DA
						Inner Join AcumuladosPorEjercicioFiscalIndem A1 On A1.IdEmp = DA.IdEmp
						Inner Join Percepciones P On P.IdPercepcion = A1.IdPercDeduc And A1.TipoPercDeduc = 'P'
						Inner Join PercsClasifPercDeducHacienda PH On PH.IdPercepcion = A1.IdPercDeduc
						Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
					Where DA.Ejercicio = @Ejercicio And A1.Ejercicio = @Ejercicio
						And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
						And DA.IdEmp Not In
							(
								Select Distinct IdEmp
								From Bajas_Borrar
								Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
							)				
				) T
			Group By ClaveConcepto, NombreConcepto, Gravar, ClaveClasifPercDeduc, DescClasifPercDeduc
			Union All
			Select ClaveConcepto, NombreConcepto, Gravar, ClaveClasifPercDeduc, DescClasifPercDeduc, Sum(Importe) Importe
			From
			(
				Select D.ClaveDeduccion ClaveConcepto, D.NombreDeduccion NombreConcepto, D.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, 
					--A1.Importe Importe
					Case
						When A1.Origen In ('DR','DN') Then A1.Importe
						Else -A1.Importe
					End Importe
				From DeclaracionesAnuales DA
					Inner Join AcumuladosPorEjercicioFiscal A1 On A1.IdEmp = DA.IdEmp
					Inner Join Deducciones D On D.IdDeduccion = A1.IdPercDeduc And A1.TipoPercDeduc = 'D'
					Inner Join DeducsClasifPercDeducHacienda PH On PH.IdDeduccion = A1.IdPercDeduc
					Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
				Where DA.Ejercicio = @Ejercicio And A1.Ejercicio = @Ejercicio
					And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
					And DA.IdEmp Not In
						(
							Select Distinct IdEmp
							From Bajas_Borrar
							Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
						)			
				Union All
				Select D.ClaveDeduccion ClaveConcepto, D.NombreDeduccion NombreConcepto, D.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, -A1.Importe Importe
				From DeclaracionesAnuales DA
					Inner Join AcumuladosPorEjercicioFiscal2 A1 On A1.IdEmp = DA.IdEmp
					Inner Join Deducciones D On D.IdDeduccion = A1.IdPercDeduc And A1.TipoPercDeduc = 'D'
					Inner Join DeducsClasifPercDeducHacienda PH On PH.IdDeduccion = A1.IdPercDeduc
					Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
				Where DA.Ejercicio = @Ejercicio And A1.Ejercicio = @Ejercicio
					And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
					And DA.IdEmp Not In
						(
							Select Distinct IdEmp
							From Bajas_Borrar
							Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
						)			
				Union All
				Select D.ClaveDeduccion ClaveConcepto, D.NombreDeduccion NombreConcepto, D.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, -A1.Importe Importe
				From DeclaracionesAnuales DA
					Inner Join AcumuladosPorEjercicioFiscalIndem A1 On A1.IdEmp = DA.IdEmp
					Inner Join Deducciones D On D.IdDeduccion = A1.IdPercDeduc And A1.TipoPercDeduc = 'D'
					Inner Join DeducsClasifPercDeducHacienda PH On PH.IdDeduccion = A1.IdPercDeduc
					Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
				Where DA.Ejercicio = @Ejercicio And A1.Ejercicio = @Ejercicio
					And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
					And DA.IdEmp Not In
						(
							Select Distinct IdEmp
							From Bajas_Borrar
							Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
						)
			) T
			Group By ClaveConcepto, NombreConcepto, Gravar, ClaveClasifPercDeduc, DescClasifPercDeduc
			) T
		Order By T.ClaveClasifPercDeduc, T.ClaveConcepto
	End
Else
	Begin
		Declare @TblQnas Table
			(IdQuincena Smallint)
			
		Insert Into @TblQnas
		Select IdQuincena
		From
			(
				Select
					Case
						When Quincena % 2 = 0 Then Quincena/2
						Else (Quincena + 1)/2
					End Mes,
					*
				From Quincenas
				Where Anio = @Ejercicio
			) T
		Where Mes = @Mes
	
		Select *
		From
			(
				Select ClaveConcepto, NombreConcepto, Gravar, ClaveClasifPercDeduc, DescClasifPercDeduc, Sum(Importe) Importe
				From
				(
					Select P.ClavePercepcion ClaveConcepto, P.NombrePercepcion NombreConcepto, P.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, 
						Case
							When A1.Origen In ('DR','DN') Then -A1.Importe
							Else A1.Importe
						End Importe, A1.IdQuincenaAplicacion
					From AcumuladosPorEjercicioFiscal A1
						Inner Join Percepciones P On P.IdPercepcion = A1.IdPercDeduc And A1.TipoPercDeduc = 'P'
						Inner Join PercsClasifPercDeducHacienda PH On PH.IdPercepcion = A1.IdPercDeduc
						Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
					Where A1.Ejercicio = @Ejercicio
						And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
						And A1.IdEmp Not In
							(
								Select Distinct IdEmp
								From Bajas_Borrar
								Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
							)				
					Union All
					Select P.ClavePercepcion ClaveConcepto, P.NombrePercepcion NombreConcepto,
						P.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, 
						A1.Importe, A1.IdQuincenaAplicacion
					From AcumuladosPorEjercicioFiscal2 A1
						Inner Join Percepciones P On P.IdPercepcion = A1.IdPercDeduc And A1.TipoPercDeduc = 'P'
						Inner Join PercsClasifPercDeducHacienda PH On PH.IdPercepcion = A1.IdPercDeduc
						Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
					Where A1.Ejercicio = @Ejercicio
						And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
						And A1.IdEmp Not In
							(
								Select Distinct IdEmp
								From Bajas_Borrar
								Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
							)				
					Union All
					Select P.ClavePercepcion ClaveConcepto, P.NombrePercepcion NombreConcepto,
						P.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, 
						A1.Importe, A1.IdQuincenaAplicacion
					From AcumuladosPorEjercicioFiscalIndem A1
						Inner Join Percepciones P On P.IdPercepcion = A1.IdPercDeduc And A1.TipoPercDeduc = 'P'
						Inner Join PercsClasifPercDeducHacienda PH On PH.IdPercepcion = A1.IdPercDeduc
						Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
					Where A1.Ejercicio = @Ejercicio
						And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
						And A1.IdEmp Not In
							(
								Select Distinct IdEmp
								From Bajas_Borrar
								Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
							)
				) T
			Where T.IdQuincenaAplicacion In
										(
											Select IdQuincena
											From @TblQnas
										)			
			Group By ClaveConcepto, NombreConcepto, Gravar, ClaveClasifPercDeduc, DescClasifPercDeduc
			Union All
			Select ClaveConcepto, NombreConcepto, Gravar, ClaveClasifPercDeduc, DescClasifPercDeduc, Sum(Importe) Importe
			From
			(
				Select D.ClaveDeduccion ClaveConcepto, D.NombreDeduccion NombreConcepto, D.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, 
					--A1.Importe Importe
					Case
						When A1.Origen In ('DR','DN') Then A1.Importe
						Else -A1.Importe
					End Importe, A1.IdQuincenaAplicacion
				From AcumuladosPorEjercicioFiscal A1
					Inner Join Deducciones D On D.IdDeduccion = A1.IdPercDeduc And A1.TipoPercDeduc = 'D'
					Inner Join DeducsClasifPercDeducHacienda PH On PH.IdDeduccion = A1.IdPercDeduc
					Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
				Where A1.Ejercicio = @Ejercicio
					And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
					And A1.IdEmp Not In
						(
							Select Distinct IdEmp
							From Bajas_Borrar
							Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
						)			
				Union All
				Select D.ClaveDeduccion ClaveConcepto, D.NombreDeduccion NombreConcepto, 
					D.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, 
					-A1.Importe Importe, A1.IdQuincenaAplicacion
				From AcumuladosPorEjercicioFiscal2 A1
					Inner Join Deducciones D On D.IdDeduccion = A1.IdPercDeduc And A1.TipoPercDeduc = 'D'
					Inner Join DeducsClasifPercDeducHacienda PH On PH.IdDeduccion = A1.IdPercDeduc
					Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
				Where A1.Ejercicio = @Ejercicio
					And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
					And A1.IdEmp Not In
						(
							Select Distinct IdEmp
							From Bajas_Borrar
							Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
						)			
				Union All
				Select D.ClaveDeduccion ClaveConcepto, D.NombreDeduccion NombreConcepto, 
					D.Gravar, CH.ClaveClasifPercDeduc, CH.DescClasifPercDeduc, 
					-A1.Importe Importe, A1.IdQuincenaAplicacion
				From AcumuladosPorEjercicioFiscalIndem A1
					Inner Join Deducciones D On D.IdDeduccion = A1.IdPercDeduc And A1.TipoPercDeduc = 'D'
					Inner Join DeducsClasifPercDeducHacienda PH On PH.IdDeduccion = A1.IdPercDeduc
					Inner Join ClasifPercDeducHacienda CH On CH.IdClasifPercDeduc = PH.IdClasifPercDeduc2
				Where A1.Ejercicio = @Ejercicio
					And @Ejercicio Between CH.AnioIniEjercicio And CH.AnioFinEjercicio
					And A1.IdEmp Not In
						(
							Select Distinct IdEmp
							From Bajas_Borrar
							Where IdQuincena In (Select IdQuincena from Quincenas where Anio = @Ejercicio)
						)
			) T
			Where T.IdQuincenaAplicacion In
										(
											Select IdQuincena
											From @TblQnas
										)			
			Group By ClaveConcepto, NombreConcepto, Gravar, ClaveClasifPercDeduc, DescClasifPercDeduc
			) T
		Order By T.ClaveClasifPercDeduc, T.ClaveConcepto	
	End</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ClaveConcepto">
          <DataField>ClaveConcepto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NombreConcepto">
          <DataField>NombreConcepto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Gravar">
          <DataField>Gravar</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ClaveClasifPercDeduc">
