  <Code>Public dim nombrevariable as String = ""

Public dim i as Integer = 0

Public Function FncCampoCabecera(byval valorCampo as String) as String
i = i  + 1
if i = 1 then
nombrevariable = valorCampo
end if
return nombrevariable
End Function</Code>
