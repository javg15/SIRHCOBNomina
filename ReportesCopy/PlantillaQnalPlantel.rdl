Declare @IdTipoSemestre2 Tinyint --18/10/2012

--18/10/2012
Select @IdTipoSemestre2 = ISNULL(ST.IdTipoSemestre,3)
From Semestres S
	Inner Join SemestresTipos ST On ST.TipoSemestre = S.TipoSemestre
Where @IdQuincena Between IdQuincenaIni And IdQuincenaFin
--18/10/2012

Declare @IdSemestre Smallint
Declare @TipoSemestre NChar(1)

Set @IdSemestre = dbo.Fn_ObtenSemestrePorIdQuincena(@IdQuincena)
Set @TipoSemestre = dbo.Fn_ObtenTipoSemestre(@IdSemestre)

Select
	C.IdCategoria, C.DescCategoria, Sum(H.Cantidad) TotalHoras
From Horas H
	Inner Join TiposDeHoraDocentes THD On THD.IdTipoHora = H.IdTipoHora
	Inner Join Categorias C On C.IdCategoria = THD.IdCategoria
Where H.TipoSemestre = @TipoSemestre And H.IdEstatusHora = 1
	And Left(dbo.Fn_ObtenQnaPorId(@IdQuincena),6) Between Left(dbo.Fn_ObtenQnaPorId(H.IdQuincenaIni),6) And Left(dbo.Fn_ObtenQnaPorId(H.IdQuincenaFin),6)
	And H.IdEmp In (
					Select Distinct EP.IdEmp
					From EmpleadosPlazas EP
						Inner Join PlazasHistoria PH On PH.IdPlaza = EP.IdPlaza
					Where Left(dbo.Fn_ObtenQnaPorId(@IdQuincena),6) Between Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigIni),6) And Left(dbo.Fn_ObtenQnaPorId(PH.IdQnaVigFin),6)
						And EP.IdTipoSemestre In (@IdTipoSemestre2,3) --18/10/2012
					)
	And H.IdPlantel In (@IdPlantel,@IdPlantel2)
Group By C.IdCategoria, C.DescCategoria
Order By TotalHoras Desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
