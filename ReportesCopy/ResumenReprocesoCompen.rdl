    <DataSet Name="dsInconsistencias">
      <Query>
        <DataSourceName>Nomina</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Anio">
            <Value>=Parameters!pAnio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IdMes">
            <Value>=Parameters!pIdMes.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select Distinct *
From
	(
	Select EDL.NumEmp, LTrim(Rtrim(E.NomEmp + ' ' + IsNull(E.ApePatEmp,'') + ' ' + IsNull(E.ApeMatEmp,''))) Nombre,
		HPC.NuevaClave132, HPC.NuevaClave401, HPC.Clave417, HPC.DifClave401 
	From HistoriaReprocesoCompensaciones HPC
		Inner Join Empleados E On E.IdEmp = HPC.IdEmp
		Inner Join EmpleadosDatosLaborales EDL On EDL.IdEmp = HPC.IdEmp
	Where HPC.Anio = @Anio And HPC.IdMes = @IdMes
		And ((HPC.NuevaClave132 &gt;= 0 And HPC.NuevaClave401 &lt;= 0 And HPC.Clave417 &lt;= 0 And HPC.DifClave401 &lt;= 0)
			Or (HPC.NuevaClave132 = 0 And HPC.NuevaClave401 = 0 And HPC.Clave417 = 0 And HPC.DifClave401 = 0)
			Or (HPC.Clave417 &gt; HPC.NuevaClave132 And HPC.NuevaClave132 &gt; 0))
	Union All
	Select Right(C.ClaveCobro, 5) NumEmp, LTrim(Rtrim(C.Nombre + ' ' + IsNull(C.ApePat,'') + ' ' + IsNull(C.ApeMat,''))) Nombre,
		HRC.NuevaClave132, HRC.NuevaClave401, HRC.Clave417, HRC.DifClave401 
	From HistoriaReprocesoCompensaciones2 HRC
		Inner Join Compensaciones2 C On C.IdEmpUnicoPorMes = HRC.IdEmp
	Where HRC.Anio = @Anio And HRC.IdMes = @IdMes And C.Anio = @Anio And C.IdMes = @IdMes
		And ((HRC.NuevaClave132 &gt;= 0 And HRC.NuevaClave401 &lt;= 0 And HRC.Clave417 &lt;= 0 And HRC.DifClave401 &lt;= 0)
			Or (HRC.NuevaClave132 = 0 And HRC.NuevaClave401 = 0 And HRC.Clave417 = 0 And HRC.DifClave401 = 0)
			Or (HRC.Clave417 &gt; HRC.NuevaClave132 And HRC.NuevaClave132 &gt; 0))
	) T
Order By Nombre</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="NumEmp">
          <DataField>NumEmp</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Nombre">
          <DataField>Nombre</DataField>
          <rd:TypeName>System.String</rd:TypeName>
