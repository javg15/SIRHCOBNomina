  <DataSets>
    <DataSet Name="dsResumen">
      <Query>
        <DataSourceName>Nomina</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Anio">
            <Value>=Parameters!pAnio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IdMes">
            <Value>=Parameters!pIdMes.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Declare @ImporteCompe Decimal (10,2)

Declare @TblCompen Table
(IdEmp Int,
IdPercDeduc Smallint,
TipoPercDeduc Nvarchar(1),
C1o2 Tinyint,
Importe Decimal(8, 2))

Create Table #TblPercDeduc
(IdEmp Int,
IdPercDeduc Smallint,
TipoPercDeduc Nvarchar(1),
C1o2 Tinyint,
Importe Decimal(8, 2))

Declare @IdQuincena Smallint

Declare @TblQnas Table
(IdQuincena Smallint)

Insert Into @TblCompen
Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Sum(Importe) Importe
From (
	Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Sum(Importe) Importe
	From (
		Select IdEmp, IdPercDeduc, TipoPercDeduc, Sum(Importe) Importe, C1o2
		From
			(
				Select IsNull(CD1.IdEmp,C1.IdEmp) IdEmp, 107 IdPercDeduc, 'P' TipoPercDeduc,
					Case
						When CD1.IdEmp Is Not Null Then C1.Importe - CD1.Importe
						Else C1.Importe
					End Importe,
					1 C1o2
				From Compensaciones C1
					Left Join CompensacionesDevoluciones CD1 On CD1.IdEmp = C1.IdEmp And CD1.Anio = C1.Anio And CD1.IdMes = C1.IdMes And CD1.Adicional = C1.Adicional
				Where C1.Anio = @Anio And C1.IdMes = @IdMes
			) T1
		Group By IdEmp, IdPercDeduc, TipoPercDeduc, C1o2
		Union All
		Select C2.IdEmpAfectado IdEmp, 107 IdPercDeduc, 'P' TipoPercDeduc, Importe, 1 C1o2
		From Compensaciones2 C2
			Inner Join EmpleadosDatosLaborales EDL On EDL.IdEmp = C2.IdEmpAfectado
		Where C2.Anio = @Anio And C2.IdMes = @IdMes
		) T 
	Group By IdEmp, IdPercDeduc, TipoPercDeduc, C1o2
	Union All
	Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Sum(Importe) Importe
	From (
		Select IdEmpUnicoPorMes IdEmp, 107 IdPercDeduc, 'P' TipoPercDeduc, Importe, 2 C1o2
		From Compensaciones2
		Where Anio =  @Anio And IdMes = @IdMes And IdEmpAfectado Is Null
		) T
	Group By IdEmp, IdPercDeduc, TipoPercDeduc, C1o2
	) T
Group By IdEmp, IdPercDeduc, TipoPercDeduc, C1o2

Select @ImporteCompe = Sum(Importe)
From @TblCompen

Delete @TblCompen
Insert Into @TblCompen
Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Sum(Importe) Importe
From (
	Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Sum(Importe) Importe
	From (
		Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Sum(Importe) Importe
		From
			(
				Select IsNull(CD1.IdEmp,C1.IdEmp) IdEmp, 107 IdPercDeduc, 'P' TipoPercDeduc,
					Case
						When CD1.IdEmp Is Not Null Then C1.Importe - CD1.Importe
						Else C1.Importe
					End Importe,
					1 C1o2
				From Compensaciones C1
					Left Join CompensacionesDevoluciones CD1 On CD1.IdEmp = C1.IdEmp And CD1.Anio = C1.Anio And CD1.IdMes = C1.IdMes And CD1.Adicional = C1.Adicional
				Where C1.Anio = @Anio And C1.IdMes = @IdMes
			) T1
		Group By IdEmp, IdPercDeduc, TipoPercDeduc, C1o2
		Union All
		Select C2.IdEmpAfectado IdEmp, 107 IdPercDeduc, 'P' TipoPercDeduc, 1 C1o2, Importe
		From Compensaciones2 C2
			Inner Join EmpleadosDatosLaborales EDL On EDL.IdEmp = C2.IdEmpAfectado
		Where C2.Anio = @Anio And C2.IdMes = @IdMes
		Union All
		Select C2.IdEmpAfectado IdEmp, 4 IdPercDeduc, 'D' TipoPercDeduc, 1 C1o2, Importe
		From Compensaciones2 C2
			Inner Join EmpleadosDatosLaborales EDL On EDL.IdEmp = C2.IdEmpAfectado
		Where C2.Anio = @Anio And C2.IdMes = @IdMes
		) T 
	Group By IdEmp, IdPercDeduc, TipoPercDeduc, C1o2
	Union All
	Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Sum(Importe) Importe
	From (
		Select IdEmpUnicoPorMes IdEmp, 107 IdPercDeduc, 'P' TipoPercDeduc, Importe, 2 C1o2
		From Compensaciones2
		Where Anio =  @Anio And IdMes = @IdMes And IdEmpAfectado Is Null
		) T
	Group By IdEmp, IdPercDeduc, TipoPercDeduc, C1o2
	) T
Group By IdEmp, IdPercDeduc, TipoPercDeduc, C1o2

Insert Into @TblQnas
Select IdQuincena
From Quincenas
Where Month(FechaDePago) = @IdMes And Year(FechaDePago) = @Anio

Delete From #TblPercDeduc

Insert Into #TblPercDeduc
Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Sum(Importe) Importe
From (
	Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Sum(Importe) Importe
	From (
		--Nómina
		Select Distinct EP.IdEmp, HP.IdPercDeduc, HP.IdQuincenaAplicacion, HP.TipoPercDeduc, 1 C1o2, HP.Importe
		From HistoriaPago HP
			Inner Join EmpleadosPlazas EP On EP.IdPlaza = HP.IdPlaza
		Where HP.IdQuincenaAplicacion In
										(Select IdQuincena
										From @TblQnas)
			And EP.IdEmp In (Select IdEmp From @TblCompen Where C1o2 = 1)
		Union All
		--Recibos
		Select R.IdEmp, RI.IdPercDeduc, RI.IdQuincenaAplicacion, RI.TipoPercDeduc, 1 C1o2, RI.Importe
		From RecibosImportes RI
			Inner Join Recibos R On R.IdRecibo = RI.IdRecibo
		Where (R.IdQuincenaAplicacion In
									(Select IdQuincena
									From @TblQnas)
					Or (Year(R.Fecha) = @Anio And Month(R.Fecha) = @IdMes))
			And R.IdEstatusRecibo = 1
			And R.IdEmp In (Select IdEmp From @TblCompen Where C1o2 = 1) 			And R.IdRecibo Not In (783)
		Union All
		Select IdEmp, IdPercDeduc, IdQuincenaAplicacion, TipoPercDeduc, C1o2, Sum(Importe) Importe
		From (
			Select IdEmp, 19 IdPercDeduc, IdQuincenaAplicacion, 'P' TipoPercDeduc, 1 C1o2, Importe
			From BonoAnualDeProductividad
			Where IdQuincenaAplicacion In (Select IdQuincena
						From @TblQnas)
				And IdEmp In (Select IdEmp From @TblCompen Where C1o2 = 1)
			) T
		Group By IdEmp, IdPercDeduc, IdQuincenaAplicacion, TipoPercDeduc, C1o2
		Union All
		Select IdEmp, IdPercDeduc, IdQuincenaAplicacion, TipoPercDeduc, C1o2, Sum(Importe) Importe
		From (
			Select IdEmp, 1 IdPercDeduc, IdQuincenaAplicacion, 'D' TipoPercDeduc, 1 C1o2, Importe
			From ImpuestoAbsorbidoPorColegio
			Where IdQuincenaAplicacion In 
						(Select IdQuincena
						From @TblQnas)
					And IdEmp In (Select IdEmp From @TblCompen Where C1o2 = 1)
			        And IdPercepcion = 19
			) T
		Group By IdEmp, IdPercDeduc, IdQuincenaAplicacion, TipoPercDeduc, C1o2
		) T
	Group By IdEmp, IdPercDeduc, TipoPercDeduc, C1o2
	Union All
	--Compensaciones
	Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Importe
	From @TblCompen
	Where C1o2 = 1
	Union All
	--Compensaciones2
	Select IdEmp, IdPercDeduc, TipoPercDeduc, C1o2, Importe
	From @TblCompen
	Where C1o2 = 2
	) T
Group By IdEmp, IdPercDeduc, TipoPercDeduc, C1o2

Declare @C401 Decimal(10,2)
Declare @C132 Decimal(10,2)
Declare @C417 Decimal(10,2)

Select @C401 = Sum(Importe)
From #TblPercDeduc
Where IdPercDeduc = 1 And TipoPercDeduc = 'D'

Select @C417 = Sum(Importe)
From #TblPercDeduc
Where IdPercDeduc = 17 And TipoPercDeduc = 'D'

Select @C132 = Sum(Importe) - IsNull(@C417,0)
From #TblPercDeduc
Where IdPercDeduc = 13 And TipoPercDeduc = 'P'

Drop Table #TblPercDeduc

Select @ImporteCompe Importe, A.C417 + B.C417 'C417', A.ISR + B.ISR ISR, A.N401 + B.N401 N401, A.N132 + B.N132 N132, @C401 C401, @C132 C132
From (
	Select 1 Registro, @ImporteCompe Importe, Sum(Clave417) 'C417', Sum(DifClave401) ISR, Sum(NuevaClave401)  N401, Sum(NuevaClave132) N132, @C401 C401, @C132 C132
	From HistoriaReprocesoCompensaciones
	Where Anio = @Anio And IdMes = @IdMes
	) A
	Inner Join (
			Select 1 Registro, Sum(Clave417) 'C417', Sum(DifClave401) ISR, Sum(NuevaClave401) N401, Sum(NuevaClave132) N132
			From HistoriaReprocesoCompensaciones2
			Where Anio = @Anio And IdMes = @IdMes
				) B On B.Registro = A.Registro</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Importe">
          <DataField>Importe</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="C417">
          <DataField>C417</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ISR">
          <DataField>ISR</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="N401">
          <DataField>N401</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="N132">
          <DataField>N132</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="C401">
          <DataField>C401</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
