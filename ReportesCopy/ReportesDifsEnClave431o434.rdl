  <DataSets>
    <DataSet Name="dsQuincena">
      <Query>
        <DataSourceName>Nomina</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdQuincena">
            <Value>=Parameters!pIdQuincena.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IdTipoReporte">
            <Value>=Parameters!pIdTipoReporte.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Declare @StrTipoReporte NVarchar(100)

If @IdTipoReporte = 1
	Begin
		Set @StrTipoReporte = 'EMPLEADOS CON DIFERENCIA EN DESCUENTO DE LA CLAVE 434 '
	End
Else
	Begin
		If @IdTipoReporte = 2
			Begin
				Set @StrTipoReporte = 'EMPLEADOS CON DIFERENCIA EN DESCUENTO DE LA CLAVE 431 '
			End
		Else
			Begin
				If @IdTipoReporte = 3
					Begin
						Set @StrTipoReporte = 'EMPLEADOS CON DIFERENCIA EN DESCUENTO DE LA CLAVE 451 '
					End			
			End	
	End

Set dateformat dmy


Select T1.*,
	@StrTipoReporte + 'EN LA QUINCENA ' + T1.Quincena TituloQuincena
From (
	Select T1.*,
		Case
									When T1.DiaInicial = 1 Then 15
			When T1.Mes In (1, 3, 5, 7, 8, 10, 12) Then 31
			When Mes In (4, 6, 9, 11) Then 30
			When dbo.Fn_ObtenSiAnioEsBisiesto(T1.Anio) = 1 Then 29
			Else 28
		End DiaFinal
	From (
		Select dbo.Fn_ObtenQnaPorId(IdQuincena) Quincena, Anio,
			Case 
				When (Quincena % 2) = 0 Then (Quincena/2)
				Else (Quincena + 1)/2
			End Mes,
			Case 
				When (Quincena % 2) = 0 Then 16
				Else 1
			End DiaInicial, FechaDePago
		From Quincenas
		Where IdQuincena = @IdQuincena) T1
	) T1
	Inner Join Meses M On M.IdMes = T1.Mes</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
