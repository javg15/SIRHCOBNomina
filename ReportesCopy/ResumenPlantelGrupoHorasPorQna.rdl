      <Query>
        <DataSourceName>Nomina</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdQuincena">
            <Value>=Parameters!pIdQuincena.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @IdQuincena SMALLINT
Declare @ClavePlantel NVarchar(2)

--SET @IdQuincena = 1995
--SET @ClavePlantel = '35'

Declare @TblSem1 Table
(Idregistro NVarchar(8), ClavePlantel NVarchar(2), DescPlantel NVarchar(50),
Grupo NVarchar(4), HorasPorGrupo Smallint)
Declare @TblSem2 Table
(Idregistro NVarchar(8), ClavePlantel NVarchar(2), DescPlantel NVarchar(50),
Grupo NVarchar(4), HorasPorGrupo Smallint)
Declare @TblSem3 Table
(Idregistro NVarchar(8), ClavePlantel NVarchar(2), DescPlantel NVarchar(50),
Grupo NVarchar(4), HorasPorGrupo Smallint)
Declare @TblFinal Table
(ClavePlantel NVarchar(2), DescPlantel NVarchar(50),
Semestre1 NVarchar(4), HorasSem1 Smallint,
Semestre2 NVarchar(4), HorasSem2 Smallint,
Semestre3 NVarchar(4), HorasSem3 Smallint,
IdZonaGeografica Tinyint)

DELETE @TblFinal

DECLARE _Cursor CURSOR FOR
	SELECT ClavePlantel
	FROM Planteles P
	Where P.IdQnaFin = 32767

OPEN _Cursor
FETCH NEXT FROM _Cursor
INTO @ClavePlantel
WHILE @@FETCH_STATUS = 0
	BEGIN
		Delete @TblSem1
		Insert Into @TblSem1
		Select 
			Convert(NVARCHAR(8), ROW_NUMBER() OVER(ORDER BY T1.Grupo Asc)) IdRegistro,
			T1.ClavePlantel, T1.DescPlantel, T1.Grupo, Sum(T1.Cantidad) HorasPorGrupo
		From
		(
			Select 
				Distinct H.IdEmp, P.ClavePlantel, P.DescPlantel, G.Grupo, H.Cantidad
			From HistoriaDetallePagoHoras HDPH
				Inner Join Horas H On H.IdHora = HDPH.IdHora
				Inner Join Planteles P On P.IdPlantel = H.IdPlantel
				Inner Join Grupos G On G.IdGrupo = H.IdGrupo
				Inner Join TiposDeNominas TN On TN.IdTipoNomina = H.IdTipoNomina
			Where HDPH.IdQuincenaAplicacion = @IdQuincena
				And P.ClavePlantel = @ClavePlantel 
				And G.Grupo Like '1%'
		) T1
		Group By T1.ClavePlantel, T1.DescPlantel, T1.Grupo

		Delete @TblSem2
		Insert Into @TblSem2
		Select 
			Convert(NVARCHAR(8), ROW_NUMBER() OVER(ORDER BY T1.Grupo Asc)) IdRegistro,
			T1.ClavePlantel, T1.DescPlantel, T1.Grupo, Sum(T1.Cantidad) HorasPorGrupo
		From
		(
			Select 
				Distinct H.IdEmp, P.ClavePlantel, P.DescPlantel, G.Grupo, H.Cantidad
			From HistoriaDetallePagoHoras HDPH
				Inner Join Horas H On H.IdHora = HDPH.IdHora
				Inner Join Planteles P On P.IdPlantel = H.IdPlantel
				Inner Join Grupos G On G.IdGrupo = H.IdGrupo
				Inner Join TiposDeNominas TN On TN.IdTipoNomina = H.IdTipoNomina
			Where HDPH.IdQuincenaAplicacion = @IdQuincena
				And P.ClavePlantel = @ClavePlantel 
				And G.Grupo Like '3%'
		) T1
		Group By T1.ClavePlantel, T1.DescPlantel, T1.Grupo

		Delete @TblSem3
		Insert Into @TblSem3
		Select 
			Convert(NVARCHAR(8), ROW_NUMBER() OVER(ORDER BY T1.Grupo Asc)) IdRegistro,
			T1.ClavePlantel, T1.DescPlantel, T1.Grupo, Sum(T1.Cantidad) HorasPorGrupo
		From
		(
			Select 
				Distinct H.IdEmp, P.ClavePlantel, P.DescPlantel, G.Grupo, H.Cantidad
			From HistoriaDetallePagoHoras HDPH
				Inner Join Horas H On H.IdHora = HDPH.IdHora
				Inner Join Planteles P On P.IdPlantel = H.IdPlantel
				Inner Join Grupos G On G.IdGrupo = H.IdGrupo
				Inner Join TiposDeNominas TN On TN.IdTipoNomina = H.IdTipoNomina
			Where HDPH.IdQuincenaAplicacion = @IdQuincena
				And P.ClavePlantel = @ClavePlantel 
				And G.Grupo Like '5%'
		) T1
		Group By T1.ClavePlantel, T1.DescPlantel, T1.Grupo

		Insert Into @TblFinal
		Select Distinct A.ClavePlantel, A.DescPlantel, A.Grupo Semestre1, A.HorasPorGrupo HorasSem1,
			IsNull(B.Grupo,'') Semestre3, IsNull(B.HorasPorGrupo,'') HorasSem3,
			IsNull(C.Grupo,'') Semestre3, IsNull(C.HorasPorGrupo,'') HorasSem5,
			P.IdZonaGeografica
		From @TblSem1 A
			Inner Join Planteles P On P.ClavePlantel = A.ClavePlantel
			Left Join @TblSem2 B On A.ClavePlantel = B.ClavePlantel And A.Idregistro = B.Idregistro
			Left Join @TblSem3 C On A.ClavePlantel = C.ClavePlantel And A.Idregistro = C.Idregistro
		--Order By A.ClavePlantel

		--Select * From @TblSem1
		--Select * From @TblSem2
		--Select * From @TblSem3
		
		FETCH NEXT FROM _Cursor
		INTO @ClavePlantel
	END

CLOSE _Cursor
DEALLOCATE _Cursor

SELECT * FROM @TblFinal</CommandText>
